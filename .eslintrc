{
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended", "plugin:jsx-a11y/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react", "react-hooks", "jsx-a11y"],
    "rules": {
      "prettier/prettier": ["error", {
        "endOfLine": "auto"
      }]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  